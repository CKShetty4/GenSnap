{
  "expo": {
    "name": "GenSnap",
    "slug": "gensnap",
    "version": "1.0.0",
    "orientation": "portrait",
    "_comment":"'icon':'./assets/images/icon.png'",
    "scheme": "gensnap",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "_comment":"'foregroundImage':'./assets/images/adaptive-icon.png'",
        "backgroundColor": "#ffffff",
        "package":"com.ckshetty.gensnap"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
